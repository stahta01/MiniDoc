<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="MiniDoc wx3.1.x - MSys2" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="plugin">
				<Option output="$(#CB.bin)/share/codeblocks/plugins/MiniDoc" prefix_auto="1" extension_auto="1" />
				<Option object_output=".objs31_msys2" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -na -ns -nd" />
				<Option host_application="../../../devel31_64/codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="-Wall" />
					<Add option="-pipe" />
					<Add option="-mthreads" />
					<Add option="-fmessage-length=0" />
					<Add option="-fexceptions" />
					<Add option="-Winvalid-pch" />
					<Add option="$(#CB_RELEASE_TYPE)" />
					<Add option="$(#CB_PCH_TYPE)" />
					<Add option="`$(WX_CONFIG) --cflags`" />
					<Add option="-DBUILDING_PLUGIN" />
					<Add option="-DwxUSE_UNICODE" />
					<Add option="-DEXPORT_FUNC" />
					<Add directory="$(#CB.include)" />
					<Add directory="$(#CB.include)/tinyxml" />
					<Add directory="$(#CB)/sdk/wxscintilla/include" />
				</Compiler>
				<Linker>
					<Add option="-mthreads" />
					<Add option="--enable-auto-import" />
					<Add option="`$(WX_CONFIG) --libs`" />
					<Add library="codeblocks" />
					<Add directory="$(#CB.bin)" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -jq9 $(#CB.bin)\share\codeblocks\MiniDoc.zip manifest.xml *.xrc" />
					<Mode after="always" />
				</ExtraCommands>
				<Environment>
					<Variable name="WX_SUFFIX" value="ud" />
					<Variable name="WX_VERSION" value="30" />
				</Environment>
			</Target>
			<Environment>
				<Variable name="CB_PCH_TYPE" value="-DCB_PRECOMP -DcbDEBUG" />
				<Variable name="WX_CONFIG" value="wx-config --version=3.1 --prefix=$(MINGW_PREFIX)" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="plugin;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-std=gnu++11" />
		</Compiler>
		<Unit filename="MicroStyledTextCtrl.cpp" />
		<Unit filename="MicroStyledTextCtrl.h" />
		<Unit filename="MiniDoc.cpp" />
		<Unit filename="MiniDoc.h" />
		<Unit filename="MiniDocConfigPanel.cpp" />
		<Unit filename="MiniDocConfigPanel.h" />
		<Unit filename="MiniDocPanel.cpp" />
		<Unit filename="MiniDocPanel.h" />
		<Unit filename="MiniStyledTextCtrl.cpp" />
		<Unit filename="MiniStyledTextCtrl.h" />
		<Unit filename="MiniStyledTextCtrlEvent.cpp" />
		<Unit filename="MiniStyledTextCtrlEvent.h" />
		<Unit filename="ToDo.txt" />
		<Unit filename="manifest.xml" />
		<Unit filename="wxsmith/MiniDocConfigPanel.wxs" />
		<Extensions>
			<envvars />
			<code_completion />
			<debugger />
			<lib_finder disable_auto="1" />
			<wxsmith version="1">
				<gui name="wxWidgets" src="" main="" init_handlers="necessary" language="CPP" />
				<resources>
					<wxPanel wxs="wxsmith/MiniDocConfigPanel.wxs" src="MiniDocConfigPanel.cpp" hdr="MiniDocConfigPanel.h" xrc="MiniDocConfigPanel.xrc" fwddecl="1" i18n="1" name="MiniDocConfigPanel" language="CPP" />
				</resources>
			</wxsmith>
			<DoxyBlocks>
				<comment_style block="0" line="0" />
				<doxyfile_project />
				<doxyfile_build />
				<doxyfile_warnings />
				<doxyfile_output />
				<doxyfile_dot />
				<general />
			</DoxyBlocks>
			<fortran_project />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
